-- Initial imagefun.cabal generated by cabal init.  For further
-- documentation, see http://haskell.org/cabal/users-guide/

name:                imagefun
version:             0.0.0.1
synopsis:            REST API for saving images for friends
description:         Backend for a simple way to copy and tag images, and save them to show friends
license:             MIT
license-file:        LICENSE
author:              Landon Gilbert-Bland
maintainer:          landogbland@gmail.com
category:            Web
build-type:          Simple
extra-source-files:  ChangeLog.md
cabal-version:       >=1.10

executable imagefun
  main-is:             Main.hs
  build-depends:       aeson == 1.2.1.0,
                       base >=4.9 && <4.10,
                       bytestring == 0.10.8.1,
                       case-insensitive == 1.2.0.7,
                       ConfigFile == 1.1.4,
                       data-default-class == 0.1.2.0,
                       fast-logger == 2.4.10,
                       MissingH == 1.4.0.1,
                       monad-control == 1.0.2.2,
                       mtl == 2.2.1,
                       opaleye == 0.5.4.0,
                       postgresql-simple == 0.5.3.0,
                       product-profunctors == 0.8.0.3,
                       resource-pool == 0.2.3.2,
                       servant == 0.11,
                       servant-server == 0.11,
                       time == 1.6.0.1,
                       transformers == 0.5.2.0,
                       wai == 3.2.1.1,
                       wai-extra == 3.0.20.0,
                       warp == 3.2.13
  hs-source-dirs:      src
  default-language:    Haskell2010
